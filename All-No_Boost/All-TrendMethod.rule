## Init settings.
# Parameter list to optimize.
param=TrendMethod
param_list=( ${param} )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Enable trade with trend.
input_set ^TradeWithTrend 1

## Optimization settings.
ea_set_optimize_param ${param} 1 255

