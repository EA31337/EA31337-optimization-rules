## Init settings.
# Parameter list to optimize.
name=MACD
param=${name}_Shift_Far
param_list=( ${param} )
## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
value=$(input_get MACD_Shift)
start=$(( $value - 2 > 0 ? $value - 2 : 0 ))
end=$(( $value + 2 ))
ea_set_optimize_param ${param} $start $end

