## Init settings.
# Parameter list to optimize.
name=MACD
param=${name}_Period_Signal
param_list=( ${param} )
## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get ${param})
ea_set_optimize_param ${param} $(( $value - 2 > 2 ? $(( $value - 2 )) : 1 )) $(( $value + 2)) 1

