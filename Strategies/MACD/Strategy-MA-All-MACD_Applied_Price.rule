## Init settings.
# Parameter list to optimize.
name=MACD
param=${name}_Applied_Price
param_list=( ${param} )
## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
ea_set_optimize_param ${param} 0 6

