## Init settings.
# Parameter list to optimize.
name=MACD
param=${name}_SignalLevel
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get ${param})
start=$( bc <<< "$value - 0.2" )
end=$(   bc <<< "$value + 0.2" )
step=0.1
ea_set_optimize_param ${param} ${start} ${end} ${step}

