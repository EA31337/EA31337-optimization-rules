## Init settings.
name=RSI
param=${name}_Period
param_list=( $param )
period=$(ea_get_period)

## EA settings.
ea_default_sets
ea_strategies_enabled 0
ea_strategy_enabled ${name}${period#M}

## Optimization settings.
value=$(input_get RSI_Period)
ea_set_optimize_param ${param} $(($value - 4)) $(($value + 4)) 2

