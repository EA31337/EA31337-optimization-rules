## Init settings.
# Parameter list to optimize.
name=RSI
param=${name}_Shift
param_list=( $param )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get $param)
ea_set_optimize_param ${param} $(( $value - 2 )) $(( $value + 2 ))

