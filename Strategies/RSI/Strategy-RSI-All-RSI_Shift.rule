## Init settings.
# Parameter list to optimize.
name=RSI
param=${name}_Shift
param_list=( $param )
## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}


## Optimization settings.
value=$(input_get $param)
input_set ^$param,F 1
input_set ^$param,1 $(( $value - 2 >= 0 ? $value - 2 : 0 ))
input_set ^$param,2 1
input_set ^$param,3 $(( $value + 2 ))

