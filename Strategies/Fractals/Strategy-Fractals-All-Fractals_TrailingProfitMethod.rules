## Init settings.
# Parameter list to optimize.
param_list=( Fractals_TrailingProfitMethod )
## EA settings.
input_set ^CloseOrderAfterXHours 72
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Reset open methods.
input_set ^Fractals.*_SignalMethod 0
# Enable strategy.
input_set ^Fractals.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Fractals_TrailingProfitMethod.
input_set ^Fractals_TrailingProfitMethod,F 1
input_set ^Fractals_TrailingProfitMethod,1 0
input_set ^Fractals_TrailingProfitMethod,2 1
input_set ^Fractals_TrailingProfitMethod,3 27

