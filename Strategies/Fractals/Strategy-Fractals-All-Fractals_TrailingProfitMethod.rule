## Init settings.
name=Fractals
param=${name}_TrailingProfitMethod
# Parameter list to optimize.
param_list=( ${param} )
period=$(ea_get_period)

## EA settings.
ea_default_sets
ea_strategies_enabled 0
ea_strategy_enabled ${name}${period#M}

## Optimization settings.
ea_set_optimize_param ${param} -27 27

