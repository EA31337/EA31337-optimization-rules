## Init settings.
# Parameter list to optimize.
name=Alligator
period=1
strategy=${name}${period}
param=${strategy}_SignalMethod
param_list=( ${param} )

## Backtest settings.
ini_set ^TestPeriod M${period}

## EA settings.
ea_strategy_default_sets ${strategy}

## Optimization settings.
ea_set_optimize_param ${param} 0 63

