## Init settings.
# Parameter list to optimize.
name=Alligator
param_list=()
param_list+=( ${name}_Shift_Jaw )
param_list+=( ${name}_Shift_Teeth )
param_list+=( ${name}_Shift_Lips )
param_list+=( ${name}_Shift )
param_list+=( ${name}_Shift_Far )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+
ea_set_neutral_trail ${name}

## Optimization settings.
for param in ${param_list[@]}; do
  value=$(input_get $param)
  ea_set_optimize_param ${param} $(($value - 1)) $(($value + 1))
done

