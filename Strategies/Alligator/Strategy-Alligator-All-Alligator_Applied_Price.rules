## Init settings.
# Parameter list to optimize.
param_list=( Alligator_Applied_Price )
## EA settings.
input_set ^CloseOrderAfterXHours 72
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Reset open methods.
input_set ^Alligator.*_SignalMethod 0
# Enable strategy.
input_set ^Alligator.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Alligator_Applied_Price.
input_set ^Alligator_Applied_Price,F 1
input_set ^Alligator_Applied_Price,1 0
input_set ^Alligator_Applied_Price,2 1
input_set ^Alligator_Applied_Price,3 6

# $ETA: 1h$
