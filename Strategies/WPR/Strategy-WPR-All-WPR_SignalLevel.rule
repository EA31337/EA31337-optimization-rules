## Init settings.
name=WPR
param=${name}_SignalLevel
param_list=( ${param} )
period=$(ea_get_period)

## EA settings.
ea_default_sets
ea_strategies_enabled 0
ea_strategy_enabled ${name}${period#M}

## Optimization settings.
value=$(input_get ${param})
ea_set_optimize_param ${param} $(( $value - 5 )) $(( $value + 5 ))

