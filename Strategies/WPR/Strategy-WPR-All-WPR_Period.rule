## Init settings.
# Parameter list to optimize.
name=WPR
param=${name}_Period
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+

## Optimization settings.
value=$(input_get ${param})
ea_set_optimize_param ${param} $(($value - 4)) $(($value + 4)) 2

