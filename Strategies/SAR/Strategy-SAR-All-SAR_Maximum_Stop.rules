## Init settings.
# Parameter list to optimize.
param_list=( SAR_Maximum_Stop )
## EA settings.
input_set ^CloseOrderAfterXHours 72
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Reset open methods.
input_set ^SAR.*_SignalMethod 0
# Enable strategy.
input_set ^SAR.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test SAR_Maximum_Stop.
input_set ^SAR_Maximum_Stop,F 1
input_set ^SAR_Maximum_Stop,1 $( echo $(input_get SAR_Maximum_Stop) - 0.5 | bc )
input_set ^SAR_Maximum_Stop,2 0.1
input_set ^SAR_Maximum_Stop,3 $( echo $(input_get SAR_Maximum_Stop) + 0.5 | bc )

