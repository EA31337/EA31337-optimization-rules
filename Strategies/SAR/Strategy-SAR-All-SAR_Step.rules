## Init settings.
# Parameter list to optimize.
name=SAR
param=${name}_Step
param_list=( $param )

## EA settings.
ea_strategy_default_sets ${name}30
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get $param)
input_set ^$param,F 1
input_set ^$param,1 $( bc <<< "if ($value - 0.10 > 0.02) $value - 0.10 else 0.02" )
input_set ^$param,2 0.01
input_set ^$param,3 $( bc <<< "$value + 0.10" )

