## Init settings.
# Parameter list to optimize.
name=SAR
param=${name}_Shift
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${name}30
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get ${param})
step=1
start=$(( $value - $step >= 0 ? $(( $value - $step )) : 0 ))
end=$(( $value + 2 ))
ea_set_optimize_param ${param} ${start} ${end} ${step}

