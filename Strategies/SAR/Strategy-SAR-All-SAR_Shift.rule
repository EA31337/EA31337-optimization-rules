## Init settings.
name=SAR
param=${name}_Shift
param_list=( ${param} )
period=$(ea_get_period)

## EA settings.
ea_default_sets
ea_strategies_enabled 0
ea_strategy_enabled ${name}${period#M}

## Optimization settings.
value=$(input_get ${param})
start=$(max $(( $value - 4 )))
end=$(( $value + 4 ))
ea_set_optimize_param ${param} ${start} ${end}

