## Init settings.
# Parameter list to optimize.
name=SAR
param=${name}_Shift
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+

## Optimization settings.
value=$(input_get ${param})
ea_set_optimize_param ${param} $(( $value - 4 )) $(( $value + 4 ))

