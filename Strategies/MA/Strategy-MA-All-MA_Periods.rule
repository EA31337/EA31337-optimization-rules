## Init settings.
# Parameter list to optimize.
strategy=MA
param_list=()
param_list+=( ${name}_Period_Fast )
param_list+=( ${name}_Period_Medium )
param_list+=( ${name}_Period_Slow )

## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}

## Optimization settings.
for param in ${param_list[@]}; do
  value=$(input_get $param)
  ea_set_optimize_param ${param} $(($value - 2)) $(($value + 2))
done

