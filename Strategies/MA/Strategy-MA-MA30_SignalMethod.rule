## Init settings.
# Parameter list to optimize.
name=MA
tf=30
PERIOD=M${tf}
strategy=${name}${tf}
param=${strategy}_SignalMethod
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${strategy}
ea_set_neutral_trail ${name}

## Optimization settings.
ea_set_optimize_param ${param} 0 127

