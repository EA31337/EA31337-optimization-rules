## Init settings.
# Parameter list to optimize.
name=Envelopes
param=${name}_MA_Method
param_list=( $param )
period=$(ea_get_period)

## EA settings.
ea_default_sets
ea_strategies_enabled 0
ea_strategy_enabled ${name}${period#M}

## Optimization settings.
ea_set_optimize_param ${param} 0 3

