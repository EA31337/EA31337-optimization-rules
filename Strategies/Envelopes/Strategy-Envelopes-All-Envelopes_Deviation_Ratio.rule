## Init settings.
# Parameter list to optimize.
name=Envelopes
param=${name}_Deviation_Ratio
param_list=( ${param} )

## EA settings.
ea_strategy_default_sets ${name}.*
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get ${param})
start=$( bc <<< "$(input_get ${param}) - 1.0" )
end=$(   bc <<< "$(input_get ${param}) + 1.0" )
step=0.1
ea_set_optimize_param ${param} ${start} ${end} ${step}

