## Init settings.
# Parameter list to optimize.
name=DeMarker
param=${name}_Period
param_list=( $param )

## EA settings.
ea_strategy_default_sets ${name}[0-9]\\+
ea_set_neutral_trail ${name}

## Optimization settings.
value=$(input_get Bands_Period)
ea_set_optimize_param ${param} $(( $value - 5 )) $(( $value + 5 ))

