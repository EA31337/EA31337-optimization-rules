## Init settings.
# Parameter list to optimize.
param_list=( BestDailyStrategyMultiplierFactor )
# Disable all optimization settings.
ea_optimize_all_set 0
# Enable boosting.
input_set Boosting_Enabled 1
# Disable account conditions.
ea_actions_enabled 0

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
input_set BestDailyStrategyMultiplierFactor,F 1
input_set BestDailyStrategyMultiplierFactor,1 $( bc <<< "$(input_get BestDailyStrategyMultiplierFactor) - 1.0" )
input_set BestDailyStrategyMultiplierFactor,2 0.1
input_set BestDailyStrategyMultiplierFactor,3 $( bc <<< "$(input_get BestDailyStrategyMultiplierFactor) + 1.0" )

# vim: set syntax=sh:
